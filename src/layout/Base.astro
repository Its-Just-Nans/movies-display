---
const { title } = Astro.props;
---

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width" />
        <meta name="generator" content={Astro.generator} />
        <title>{title}</title>
        <style>
            body {
                margin: 0;
            }
        </style>
    </head>
    <body>
        <slot />
    </body>
</html>
